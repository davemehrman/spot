<h1>Sign in to view your spots</h1>
<p>
	<%= form_tag sessions_create_path do %>
	<p><%= label_tag "Email:" %><%= text_field_tag :email %></p>
	<p><%= label_tag "Password:" %><%= password_field_tag :password %></p>
	<p><%= submit_tag "Sign In" %></p>
	<% end %>
	<%= link_to "cancel", root_url %>
</p>
